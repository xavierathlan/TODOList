        android:gravity="center"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_alarm"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="80dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonTwo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_balance"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="278dp"
        tools:layout_editor_absoluteY="80dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonThree"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_box"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="86dp"
        tools:layout_editor_absoluteY="16dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonFour"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_tree"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="80dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonFive"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_air"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="16dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonSix"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_units"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="80dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonSeven"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_suite"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="16dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonEight"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:insetTop="0dp"
        android:insetBottom="0dp"
        app:icon="@drawable/ic_airplay"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="16dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/buttonNine"
        android:layout_width="48dp"
        android:layout_height="48dp"
